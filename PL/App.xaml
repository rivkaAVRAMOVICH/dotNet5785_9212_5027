<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri=" VolunteersManagementWindow.xaml">
    <Application.Resources>
        <CollectionViewSource x:Key="AssignmentCollectionKey" Source="{Binding AssignmentCollection}" />
        <CollectionViewSource x:Key="callCollectionKey" Source="{Binding CallCollection}" />
        <CollectionViewSource x:Key="VolunteerCollectionKey" Source="{Binding VolunteerCollection}" />
        <CollectionViewSource x:Key="RolesCollectionKey" Source="{Binding RolesCollection}" />
        <CollectionViewSource x:Key="DistanceTypesCollectionKey" Source="{Binding DistanceTypesCollection}" />
    </Application.Resources>
</Application>